<%= stylesheet_link_tag "user_index.css" %>
<h1>Display All the users</h1>
<% @users.each do |user| %>
    <div class='user'>
        <div class="face-container">
          <img 
          class="face" 
          src=<%= user.Photo %>
          alt="face" />
        </div>
        <div class="details">
            <h1><%= user.Name %></h1>
            <h3>Number of posts : <%= user.PostsCounter %></h3>
        </div>
    </div>
<% end %>
